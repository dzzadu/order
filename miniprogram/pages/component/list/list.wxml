    <view class="list-box">
        <view class="newest-list" wx:for="{{goodsList}}" wx:key="index">
            <navigator url="../details/details?index={{item.id-1}}&classify={{item.classify}}">
                <image src="{{item.image}}"></image>
                <view class="newest-text"><text>{{item.title}}</text></view>
                <view class="newest-text"><text>￥ {{item.price}}</text></view>
            </navigator>
        </view>
