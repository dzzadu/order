.main{
  font-size: 32rpx;
}
.back{
  color: #fff;
  padding: 20rpx;
  text-align: center;
  width: 650rpx;
  margin: 30rpx auto;
  background-color: #AB956D;
}
.back_click{
  background-color: #af822d;
}
.value{
  background-color: #fff;
  color: #838383;
  margin-bottom: 30rpx;
  padding: 20rpx;
}
.value view{
  height: 50rpx;
  line-height: 50rpx;
}
.time{
  padding-bottom: 20rpx;
  margin-bottom: 20rpx;
  border-bottom: 1rpx solid #ededed;
}
.num {
  color: #ff2600;
}
.list{
  background-color: #fff;

}
.orders-list{
  display: flex;
  justify-content: space-between;
  height: 200rpx;
  border-bottom: 1rpx solid #ededed;
}
.list_img image{
  margin: 10rpx;
  width: 180rpx;
  height: 180rpx;
}
.list_content{
  flex: 1;
  margin-left: 20rpx;
  line-height: 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.list_content_left{
  flex: 1;
}
.list_content_right{
  margin-right: 30rpx;
}
.total{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx;
}